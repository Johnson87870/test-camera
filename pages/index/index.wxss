/**index.wxss**/

.take {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  position: absolute;
  bottom: 10px;
  left: 50%;
  transform: translateX(-50%);
}

.rectangle {
  width: 260px;
  height: 150px;
  border: 2px green solid;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.tips {
  color: #ffffff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: 95px;
  transform: translate(-50%,-50%);
}